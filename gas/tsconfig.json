{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "types": ["google-apps-script"],
    "noEmit": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
  },
  "include": ["src"]
}
